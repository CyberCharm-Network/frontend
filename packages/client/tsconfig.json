{
  "compilerOptions": {
    "strict": true,
    "target": "ESNext",
    "module": "ESNext",
    "moduleResolution": "node",
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "jsx": "preserve",
    "jsxImportSource": "solid-js",
    "types": ["vite/client", "vite-plugin-solid-svg/types"],
    "noEmit": true,
    "isolatedModules": true,
    "resolveJsonModule": true,
    "baseUrl": ".",
    "paths": {
      "@revolt/app": ["components/app"],
      // "@app/*": ["components/app/*"],
      "@revolt/auth": ["components/auth"],
      // "@auth/*": ["components/auth/*"],
      "@revolt/client": ["components/client"],
      // "@client/*": ["components/client/*"],
      "@revolt/common": ["components/common"],
      // "@common/*": ["components/common/*"],
      "@revolt/i18n": ["components/i18n"],
      // "@i18n/*": ["components/i18n/*"],
      "@revolt/keybinds": ["components/keybinds"],
      // "@keybinds/*": ["components/keybinds/*"],
      "@revolt/markdown": ["components/markdown"],
      // "@markdown/*": ["components/markdown/*"],
      "@revolt/modal": ["components/modal"],
      // "@modal/*": ["components/modal/*"],
      "@revolt/routing": ["components/routing"],
      // "@routing/*": ["components/routing/*"],
      "@revolt/state": ["components/state"],
      // "@state/*": ["components/state/*"],
      "@revolt/ui": ["components/ui"]
      // "@ui/*": ["components/ui/*"]
    }
  }
}
